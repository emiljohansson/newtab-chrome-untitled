{
  "name": "o-newtab-chrome",
  "version": "0.0.0",
  "description": "x",
  "main": "index.js",
  "scripts": {
    "start": "webpack --watch",
    "test": "NODE_PATH='.' nyc ava"
  },
  "repository": {
    "type": "git",
    "url": "x"
  },
  "keywords": [
    "newtab",
    "chrome"
  ],
  "author": "Emil Johansson <emiljohansson.se@gmail.com>",
  "license": "MIT",
  "ava": {
    "files": [
      "!bundle.*",
      "!test/helpers/**/*.js",
      "test/**/*.js"
    ],
    "require": [
      "./test/helpers/setup-test-env.js"
    ]
  },
  "nyc": {
    "include": [
      "lib/**/*.js",
      "core/**/*.js",
      "apps/**/*.js"
    ],
    "exclude": [
      "bundle.*"
    ],
    "extension": [
      ".js"
    ],
    "require": [
      "babel-register"
    ],
    "reporter": "html"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-env": "^3.2.0",
    "nyc": "^11.2.1",
    "sinon": "^3.3.0",
    "webpack": "^3.5.4"
  },
  "dependencies": {
    "jss": "^9.0.0",
    "jss-preset-default": "^4.0.1",
    "keyboard-handler": "^1.1.2",
    "lodash": "^4.17.4",
    "moment-timezone": "^0.5.13",
    "redux": "^3.7.2",
    "rxjs": "^5.4.3",
    "virtual-dom": "^2.1.1",
    "weather": "git+https://github.com/emiljohansson/weather.git"
  }
}
