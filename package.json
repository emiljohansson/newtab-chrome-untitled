{
  "name": "o-newtab-chrome",
  "version": "3.0.0",
  "description": "x",
  "main": "index.js",
  "scripts": {
    "start": "webpack --watch",
    "test-old": "NODE_PATH='./src/' nyc ava",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "standard"
  },
  "repository": {
    "type": "git",
    "url": "x"
  },
  "keywords": [
    "newtab",
    "chrome"
  ],
  "author": "Emil Johansson <emiljohansson.se@gmail.com>",
  "license": "MIT",
  "ava": {
    "files": [
      "!dist",
      "!test/helpers/**/*.js",
      "test/**/*.js"
    ],
    "source": [
      "test/**/*.js",
      "src/**/*.js",
      "!dist/**/*"
    ],
    "require": [
      "esm",
      "babel-register",
      "./test/helpers/setup-test-env.js"
    ]
  },
  "nyc": {
    "reporter": "html"
  },
  "babel": {
    "presets": [
      "@ava/stage-4"
    ]
  },
  "standard": {
    "ignore": [
      "test/helpers/webcomponents-lite.js"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js}",
      "!__test__/**/*",
      "!test/**/*",
      "!**/node_modules/**"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testMatch": [
      "**/test/**/Subject.js"
    ],
    "globals": {},
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ],
    "moduleNameMapper": {},
    "transformIgnorePatterns": [
      "/node_modules/(?!lodash-es).+\\.js$"
    ]
  },
  "devDependencies": {
    "ava": "0.25.0",
    "babel-jest": "23.6.0",
    "browser-env": "3.2.0",
    "esm": "3.0.34",
    "hoek": "4.2.1",
    "jest": "23.6.0",
    "nyc": "11.4.1",
    "qs": "6.5.2",
    "sinon": "5.0.7",
    "standard": "11.0.1",
    "webpack": "4.28.0",
    "webpack-cli": "3.1.2"
  },
  "dependencies": {
    "keyboard-handler": "2.0.0",
    "keyboard-key-code": "2.0.1",
    "lodash-es": "4.17.10",
    "weather": "git+https://github.com/emiljohansson/weather.git"
  }
}
