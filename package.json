    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "browser-env": "3.2.0",
    "nyc": "11.4.1",
    "sinon": "3.3.0",
    "standard": "^11.0.1",
    "webpack": "3.5.4"
  },
  "dependencies": {
    "array-movetoend": "1.0.0",
    "array-removeat": "1.0.0",
