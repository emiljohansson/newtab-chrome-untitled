{
  "name": "o-newtab-chrome",
  "version": "3.0.0",
  "description": "x",
  "main": "index.js",
  "scripts": {
    "start": "webpack --watch",
    "test-old": "NODE_PATH='./src/' nyc ava",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "npm run test:coverage -- --coverageReporters=text-summary",
    "lint": "standard",
    "ts:check": "tsc --noEmit",
    "ts:lint": "tslint --project tsconfig.json --config tslint.json",
    "validate": "npm run validate:code && npm run test:ci",
    "validate:code": "npm run ts:check && npm run ts:lint"
  },
  "repository": {
    "type": "git",
    "url": "x"
  },
  "keywords": [
    "newtab",
    "chrome"
  ],
  "author": "Emil Johansson <emiljohansson.se@gmail.com>",
  "license": "MIT",
  "ava": {
    "files": [
      "!dist",
      "!test/helpers/**/*.js",
      "test/**/*.js"
    ],
    "source": [
      "test/**/*.js",
      "src/**/*.js",
      "!dist/**/*"
    ],
    "require": [
      "esm",
      "babel-register",
      "./test/helpers/setup-test-env.js"
    ]
  },
  "nyc": {
    "reporter": "html"
  },
  "babel": {
    "presets": [
      "@ava/stage-4"
    ]
  },
  "standard": {
    "ignore": [
      "test/helpers/webcomponents-lite.js"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js}",
      "**/*.{ts}",
      "!**/*.d.ts",
      "!src/*.ts",
      "!__test__/**/*",
      "!test/**/*",
      "!**/node_modules/**"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "testMatch": [
      "**/test/**/*.ts"
    ],
    "globals": {
      "ts-jest": {
        "useBabelrc": true,
        "tsConfigFile": "tsconfig.json"
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ],
    "moduleNameMapper": {},
    "transformIgnorePatterns": [
      "/node_modules/(?!lodash-es).+\\.js$"
    ]
  },
  "devDependencies": {
    "@types/jest": "23.3.10",
    "@types/sinon": "7.0.2",
    "ava": "0.25.0",
    "babel-jest": "23.6.0",
    "browser-env": "3.2.0",
    "esm": "3.0.34",
    "hoek": "4.2.1",
    "jest": "23.6.0",
    "nyc": "11.4.1",
    "qs": "6.5.2",
    "sinon": "5.0.7",
    "standard": "11.0.1",
    "ts-jest": "23.10.5",
    "tslint": "5.12.0",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "3.2.2",
    "webpack": "4.28.0",
    "webpack-cli": "3.1.2"
  },
  "dependencies": {
    "keyboard-handler": "2.0.0",
    "keyboard-key-code": "2.0.1",
    "lodash-es": "4.17.10",
    "weather": "git+https://github.com/emiljohansson/weather.git"
  }
}
