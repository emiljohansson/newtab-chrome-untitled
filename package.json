    "jss-preset-default": "4.0.1",
    "keyboard-handler": "1.1.2",
    "keyboard-key-code": "^1.0.1",
    "lodash": "4.17.4",
    "moment-timezone": "0.5.13",
    "qs": "6.5.1",
    "rxjs": "5.4.3",
    "uglify-js": "3.3.7",
    "weather": "git+https://github.com/emiljohansson/weather.git"
