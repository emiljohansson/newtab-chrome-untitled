{
  "name": "o-newtab-chrome",
  "version": "4.0.0",
  "description": "x",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --env.NODE_ENV=local --env.development --inline",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "npm run test:coverage -- --coverageReporters=text-summary",
    "lint": "standard",
    "ts:check": "tsc --noEmit",
    "ts:lint": "tslint -p tsconfig.json -c tslint.json",
    "validate": "npm run validate:code && npm run test:ci",
    "validate:code": "npm run ts:check && npm run ts:lint"
  },
  "repository": {
    "type": "git",
    "url": "x"
  },
  "keywords": [
    "newtab",
    "chrome"
  ],
  "author": "Emil Johansson <emiljohansson.se@gmail.com>",
  "license": "MIT",
  "ava": {
    "files": [
      "!dist",
      "!test/helpers/**/*.js",
      "test/**/*.js"
    ],
    "source": [
      "test/**/*.js",
      "src/**/*.js",
      "!dist/**/*"
    ],
    "require": [
      "esm",
      "babel-register",
      "./test/helpers/setup-test-env.js"
    ]
  },
  "nyc": {
    "reporter": "html"
  },
  "babelx": {
    "presets": [
      "@ava/stage-4"
    ]
  },
  "standard": {
    "ignore": [
      "test/helpers/webcomponents-lite.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "23.3.10",
    "@types/lodash": "4.14.119",
    "@types/sinon": "7.0.2",
    "babel-jest": "24.9.0",
    "browser-env": "3.2.0",
    "jest": "24.9.0",
    "request": "2.88.0",
    "sinon": "5.0.7",
    "standard": "11.0.1",
    "ts-jest": "24.2.0",
    "ts-loader": "5.3.1",
    "tslint": "5.12.0",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "3.2.2",
    "webpack": "4.28.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.10.0"
  },
  "dependencies": {
    "keyboard-handler": "2.0.0",
    "keyboard-key-code": "2.0.1",
    "lodash": "^4.17.19"
  }
}
