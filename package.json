      "test/**/*.js"
    ],
    "require": [
      "babel-core/register",
      "./test/helpers/setup-test-env.js"
    ]
  },
  "nyc": {
    "reporter": "html"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "ignore": "test.js",
